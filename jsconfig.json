{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@context/*": ["src/context/*"],
            "@constants/*": ["src/constants/*"],
            "@helpers/*": ["src/helpers/*"],
            "@hooks/*": ["src/hooks/*"],
            "@middlewares/*": ["src/middlewares/*"],
            "@pages/*": ["src/pages/*"],
            "@partials/*": ["src/partials/*"],
            "@private/*": ["src/private/*"],
            "@routes/*": ["src/routes/*"],
            "@services/*": ["src/services/*"],
            "@stores/*": ["src/stores/*"],
            "@styles/*": ["src/styles/*"],
            "@tests/*": ["src/tests/*"],
            "@translations/*": ["src/translations/*"],
            "@utils/*": ["src/utils/*"],
        }
    },
    "include": ["src"]
}
  